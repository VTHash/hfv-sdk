{
  "name": "hfv-sdk",
  "version": "0.1.5",
  "description": "HFV SDK for bridge + prices + chain metadata",
  "author": "HFV Protocol",
  "license": "MIT",

  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",

  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },

  "files": ["dist"],

  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch"
  },

  "dependencies": {
    "axios": "^1.13.2"
  },

  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0",
    "@types/node": "^22.0.0"
  },

  "keywords": [
    "web3",
    "sdk",
    "bridge",
    "wormhole",
    "hfv"
  ],

  "repository": {
    "type": "git",
    "url": "git+https://github.com/VTHash/hfv-sdk.git"
  },

  "publishConfig": {
    "access": "public"
  }
}
